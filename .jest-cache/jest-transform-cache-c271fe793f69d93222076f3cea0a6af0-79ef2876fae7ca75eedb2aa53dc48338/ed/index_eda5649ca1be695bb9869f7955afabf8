339c96124b54ec96b93e8653c62c63ad
const core = require('@actions/core');
const github = require('@actions/github');
const exec = require('@actions/exec');
const fs = require('fs');
const path = require('path');
const axios = require('axios');
async function run() {
  try {
    // üè∞ GnFortress Î∏åÎûúÎî©
    core.info('üè∞ GnFortress Docker Security Scanner');
    core.info('‚ö° Enterprise-grade security scanning in 5 minutes');
    core.info('');

    // ÏûÖÎ†• ÌååÎùºÎØ∏ÌÑ∞ Í∞ÄÏ†∏Ïò§Í∏∞
    const image = core.getInput('image', {
      required: true
    });
    const slackWebhook = core.getInput('slack-webhook');
    const githubToken = core.getInput('github-token');
    const severityThreshold = core.getInput('severity-threshold') || 'MEDIUM';
    const failOnCritical = core.getInput('fail-on-critical') === 'true';
    const outputFormat = core.getInput('output-format') || 'table';
    const trivyVersion = core.getInput('trivy-version') || 'latest';
    const cacheEnabled = core.getInput('cache-enabled') !== 'false';
    core.info(`üîç Scanning Docker image: ${image}`);
    core.info(`üìä Severity threshold: ${severityThreshold}`);
    core.info(`‚öôÔ∏è  Output format: ${outputFormat}`);
    core.info(`üîß Trivy version: ${trivyVersion}`);
    core.info(`üíæ Cache enabled: ${cacheEnabled}`);
    core.info('');

    // Trivy ÏÑ§Ïπò
    core.info('üöÄ Installing Trivy scanner...');
    await installTrivy(trivyVersion);

    // Docker Ïù¥ÎØ∏ÏßÄ pull (ÌïÑÏöîÌïú Í≤ΩÏö∞)
    core.info(`üì• Pulling Docker image: ${image}`);
    await pullDockerImage(image);

    // Î≥¥Ïïà Ïä§Ï∫î Ïã§Ìñâ
    core.info('üîç Running security scan...');
    const scanResults = await runTrivyScan(image, severityThreshold, outputFormat, cacheEnabled);

    // Ïä§Ï∫î Í≤∞Í≥º Ï≤òÎ¶¨
    core.info('üìä Processing scan results...');
    const processedResults = await processScanResults(scanResults, severityThreshold);

    // Í≤∞Í≥º Ï∂úÎ†• ÏÑ§Ï†ï
    core.setOutput('scan-result', JSON.stringify(processedResults));
    core.setOutput('vulnerability-count', processedResults.vulnerabilityCount.toString());
    core.setOutput('critical-count', processedResults.criticalCount.toString());
    core.setOutput('high-count', processedResults.highCount.toString());
    core.setOutput('medium-count', processedResults.mediumCount.toString());
    core.setOutput('low-count', processedResults.lowCount.toString());
    core.setOutput('scan-status', processedResults.scanStatus);

    // Î¶¨Ìè¨Ìä∏ ÌååÏùºÏù¥ ÏÉùÏÑ±ÎêòÏóàÎã§Î©¥ URL ÏÑ§Ï†ï
    if (processedResults.reportPath) {
      const reportUrl = `${github.context.serverUrl}/${github.context.repo.owner}/${github.context.repo.repo}/actions/runs/${github.context.runId}`;
      core.setOutput('report-url', reportUrl);
    }

    // Slack ÏïåÎ¶º Ï†ÑÏÜ°
    if (slackWebhook) {
      core.info('üì± Sending Slack notification...');
      await sendSlackNotification(slackWebhook, processedResults, image);
    }

    // GitHub PR ÏΩîÎ©òÌä∏ Ï∂îÍ∞Ä
    if (githubToken && github.context.eventName === 'pull_request') {
      core.info('üí¨ Adding PR comment...');
      await addPRComment(githubToken, processedResults, image);
    }

    // Í≤∞Í≥º Ï∂úÎ†•
    core.info('');
    core.info('‚úÖ Security scan completed successfully!');
    core.info(`üìä Found ${processedResults.vulnerabilityCount} vulnerabilities:`);
    core.info(`   üî¥ Critical: ${processedResults.criticalCount}`);
    core.info(`   üü† High: ${processedResults.highCount}`);
    core.info(`   üü° Medium: ${processedResults.mediumCount}`);
    core.info(`   üü¢ Low: ${processedResults.lowCount}`);
    core.info('');
    core.info('üè∞ Powered by GnFortress - Enterprise Cloud Security');

    // Critical Ï∑®ÏïΩÏ†êÏù¥ ÏûàÍ≥† fail-on-criticalÏù¥ trueÏù∏ Í≤ΩÏö∞ Ïã§Ìå® Ï≤òÎ¶¨
    if (failOnCritical && processedResults.criticalCount > 0) {
      core.setFailed(`‚ùå Critical vulnerabilities found: ${processedResults.criticalCount}`);
    }
  } catch (error) {
    core.setFailed(`‚ùå Action failed with error: ${error.message}`);
  }
}

// Trivy ÏÑ§Ïπò Ìï®Ïàò
async function installTrivy(version = 'latest') {
  try {
    // TrivyÍ∞Ä Ïù¥ÎØ∏ ÏÑ§ÏπòÎêòÏñ¥ ÏûàÎäîÏßÄ ÌôïÏù∏
    try {
      await exec.exec('trivy', ['version'], {
        silent: true
      });
      core.info('‚úÖ Trivy is already installed');
      return;
    } catch {
      // TrivyÍ∞Ä ÏÑ§ÏπòÎêòÏñ¥ ÏûàÏßÄ ÏïäÏùå, ÏÑ§Ïπò ÏßÑÌñâ
    }
    const os = process.platform;
    const arch = process.arch === 'x64' ? 'amd64' : process.arch;
    let downloadUrl;
    let fileName;
    if (version === 'latest') {
      // GitHub releases APIÏóêÏÑú ÏµúÏã† Î≤ÑÏ†Ñ Í∞ÄÏ†∏Ïò§Í∏∞
      const latestVersion = await getLatestTrivyVersion();
      version = latestVersion;
    }
    if (os === 'linux') {
      fileName = `trivy_${version}_Linux-${arch}.tar.gz`;
      downloadUrl = `https://github.com/aquasecurity/trivy/releases/download/v${version}/${fileName}`;
    } else if (os === 'darwin') {
      fileName = `trivy_${version}_macOS-${arch}.tar.gz`;
      downloadUrl = `https://github.com/aquasecurity/trivy/releases/download/v${version}/${fileName}`;
    } else {
      throw new Error(`Unsupported platform: ${os}`);
    }
    core.info(`üì• Downloading Trivy ${version} for ${os}-${arch}...`);

    // Trivy Îã§Ïö¥Î°úÎìú
    await exec.exec('curl', ['-L', '-o', fileName, downloadUrl]);

    // ÏïïÏ∂ï Ìï¥Ï†ú
    await exec.exec('tar', ['-xzf', fileName]);

    // Ïã§Ìñâ Í∂åÌïú Î∂ÄÏó¨ Î∞è PATHÏóê Ï∂îÍ∞Ä
    await exec.exec('chmod', ['+x', 'trivy']);
    await exec.exec('sudo', ['mv', 'trivy', '/usr/local/bin/']);

    // Ï†ïÎ¶¨
    await exec.exec('rm', ['-f', fileName]);
    core.info('‚úÖ Trivy installation completed');

    // ÏÑ§Ïπò ÌôïÏù∏
    await exec.exec('trivy', ['version']);
  } catch (error) {
    throw new Error(`Failed to install Trivy: ${error.message}`);
  }
}

// ÏµúÏã† Trivy Î≤ÑÏ†Ñ Í∞ÄÏ†∏Ïò§Í∏∞
async function getLatestTrivyVersion() {
  try {
    const response = await axios.get('https://api.github.com/repos/aquasecurity/trivy/releases/latest', {
      headers: {
        'User-Agent': 'GnFortress-Docker-Scanner'
      }
    });
    const version = response.data.tag_name.replace(/^v/, '');
    return version;
  } catch (error) {
    core.warning(`‚ö†Ô∏è Failed to get latest Trivy version: ${error.message}`);
    return '0.49.1'; // ÏïàÏ†ÑÌïú Í∏∞Î≥∏Í∞í
  }
}

// Docker Ïù¥ÎØ∏ÏßÄ pull
async function pullDockerImage(image) {
  try {
    await exec.exec('docker', ['pull', image]);
    core.info(`‚úÖ Successfully pulled image: ${image}`);
  } catch (error) {
    core.warning(`‚ö†Ô∏è Failed to pull image ${image}: ${error.message}`);
    core.info('‚ÑπÔ∏è Continuing with scan - image might already exist locally');
  }
}

// Trivy Ïä§Ï∫î Ïã§Ìñâ
async function runTrivyScan(image, threshold, format, cacheEnabled) {
  try {
    const outputFile = 'trivy-results.json';
    const args = ['image', '--format', 'json', '--output', outputFile, '--severity', threshold];
    if (!cacheEnabled) {
      args.push('--no-cache');
    }

    // Ï∂îÍ∞Ä Ïä§Ï∫î ÏòµÏÖò
    args.push('--quiet');
    args.push('--exit-code', '0'); // Ï∑®ÏïΩÏ†êÏù¥ Î∞úÍ≤¨ÎêòÏñ¥ÎèÑ exit code 0ÏúºÎ°ú ÏÑ§Ï†ï

    args.push(image);
    await exec.exec('trivy', args);

    // Í≤∞Í≥º ÌååÏùº ÏùΩÍ∏∞
    const resultsJson = fs.readFileSync(outputFile, 'utf8');
    const results = JSON.parse(resultsJson);

    // Ï∂îÍ∞Ä Ìè¨Îß∑ÏúºÎ°ú Í≤∞Í≥º ÏÉùÏÑ± (table, sarif Îì±)
    if (format !== 'json') {
      const formatOutputFile = `trivy-results.${format}`;
      const formatArgs = ['image', '--format', format, '--output', formatOutputFile, '--severity', threshold, '--quiet', image];
      await exec.exec('trivy', formatArgs);
    }
    return results;
  } catch (error) {
    throw new Error(`Trivy scan failed: ${error.message}`);
  }
}

// Ïä§Ï∫î Í≤∞Í≥º Ï≤òÎ¶¨
async function processScanResults(scanResults, threshold) {
  try {
    let vulnerabilityCount = 0;
    let criticalCount = 0;
    let highCount = 0;
    let mediumCount = 0;
    let lowCount = 0;
    let scanStatus = 'success';

    // Trivy Í≤∞Í≥º Íµ¨Ï°∞ Î∂ÑÏÑù
    if (scanResults && scanResults.Results) {
      for (const result of scanResults.Results) {
        if (result.Vulnerabilities) {
          for (const vuln of result.Vulnerabilities) {
            vulnerabilityCount++;
            switch (vuln.Severity) {
              case 'CRITICAL':
                criticalCount++;
                break;
              case 'HIGH':
                highCount++;
                break;
              case 'MEDIUM':
                mediumCount++;
                break;
              case 'LOW':
                lowCount++;
                break;
            }
          }
        }
      }
    }

    // Ïä§Ï∫î ÏÉÅÌÉú Í≤∞Ï†ï
    if (criticalCount > 0) {
      scanStatus = 'failure';
    } else if (highCount > 0) {
      scanStatus = 'warning';
    }

    // ÏÉÅÏÑ∏ Ï†ïÎ≥¥ Ï∂îÍ∞Ä
    const processedResults = {
      vulnerabilityCount,
      criticalCount,
      highCount,
      mediumCount,
      lowCount,
      scanStatus,
      scanTimestamp: new Date().toISOString(),
      detailedResults: scanResults,
      reportPath: 'trivy-results.json'
    };

    // Í≤∞Í≥º ÏöîÏïΩ Î°úÍ∑∏
    core.info(`üìä Scan Summary:`);
    core.info(`   Total vulnerabilities: ${vulnerabilityCount}`);
    core.info(`   Critical: ${criticalCount}`);
    core.info(`   High: ${highCount}`);
    core.info(`   Medium: ${mediumCount}`);
    core.info(`   Low: ${lowCount}`);
    return processedResults;
  } catch (error) {
    throw new Error(`Failed to process scan results: ${error.message}`);
  }
}

// Slack ÏïåÎ¶º Ï†ÑÏÜ°
async function sendSlackNotification(webhookUrl, results, imageName) {
  try {
    const statusEmoji = {
      'success': '‚úÖ',
      'warning': '‚ö†Ô∏è',
      'failure': '‚ùå'
    };
    const colorMap = {
      'success': 'good',
      'warning': 'warning',
      'failure': 'danger'
    };
    const emoji = statusEmoji[results.scanStatus] || 'üìä';
    const color = colorMap[results.scanStatus] || 'good';
    const payload = {
      username: 'GnFortress Security Scanner',
      icon_emoji: ':shield:',
      attachments: [{
        color: color,
        title: `${emoji} Docker Security Scan Results`,
        fields: [{
          title: 'Image',
          value: imageName,
          short: true
        }, {
          title: 'Status',
          value: results.scanStatus.toUpperCase(),
          short: true
        }, {
          title: 'Total Vulnerabilities',
          value: results.vulnerabilityCount.toString(),
          short: true
        }, {
          title: 'Critical',
          value: `üî¥ ${results.criticalCount}`,
          short: true
        }, {
          title: 'High',
          value: `üü† ${results.highCount}`,
          short: true
        }, {
          title: 'Medium',
          value: `üü° ${results.mediumCount}`,
          short: true
        }],
        footer: 'GnFortress Docker Security Scanner',
        ts: Math.floor(Date.now() / 1000)
      }]
    };
    await axios.post(webhookUrl, payload, {
      headers: {
        'Content-Type': 'application/json'
      }
    });
    core.info('‚úÖ Slack notification sent successfully');
  } catch (error) {
    core.warning(`‚ö†Ô∏è Failed to send Slack notification: ${error.message}`);
  }
}

// GitHub PR ÏΩîÎ©òÌä∏ Ï∂îÍ∞Ä
async function addPRComment(token, results, imageName) {
  try {
    const octokit = github.getOctokit(token);
    const context = github.context;
    if (context.eventName !== 'pull_request') {
      core.info('‚ÑπÔ∏è Not a pull request event, skipping PR comment');
      return;
    }
    const statusEmoji = {
      'success': '‚úÖ',
      'warning': '‚ö†Ô∏è',
      'failure': '‚ùå'
    };
    const emoji = statusEmoji[results.scanStatus] || 'üìä';
    const comment = `## ${emoji} Docker Security Scan Results
    
**Image:** \`${imageName}\`  
**Status:** ${results.scanStatus.toUpperCase()}  
**Scan Time:** ${results.scanTimestamp}

### üìä Vulnerability Summary
| Severity | Count |
|----------|-------|
| üî¥ Critical | ${results.criticalCount} |
| üü† High | ${results.highCount} |
| üü° Medium | ${results.mediumCount} |
| üü¢ Low | ${results.lowCount} |
| **Total** | **${results.vulnerabilityCount}** |

### üè∞ Powered by GnFortress
Enterprise-grade security scanning in 5 minutes

${results.criticalCount > 0 ? '‚ö†Ô∏è **Critical vulnerabilities detected!** Please review and address these issues before merging.' : ''}
${results.vulnerabilityCount === 0 ? 'üéâ No vulnerabilities found! Your image is secure.' : ''}
    `;
    await octokit.rest.issues.createComment({
      owner: context.repo.owner,
      repo: context.repo.repo,
      issue_number: context.payload.pull_request.number,
      body: comment
    });
    core.info('‚úÖ PR comment added successfully');
  } catch (error) {
    core.warning(`‚ö†Ô∏è Failed to add PR comment: ${error.message}`);
  }
}

// Action Ïã§Ìñâ
run();

// ===========================================
// ÌÖåÏä§Ìä∏Ïö© Export (ÌîÑÎ°úÎçïÏÖòÏóêÏÑúÎäî ÏÇ¨Ïö©ÎêòÏßÄ ÏïäÏùå)
// ===========================================
if (process.env.NODE_ENV === 'test') {
  module.exports = {
    run,
    installTrivy,
    getLatestTrivyVersion,
    pullDockerImage,
    runTrivyScan,
    processScanResults,
    sendSlackNotification,
    addPRComment
  };
}
