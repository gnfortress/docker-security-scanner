      for (const image of images) {
        global.setupMocks(); // ê° í…ŒìŠ¤íŠ¸ë§ˆë‹¤ mock ì´ˆê¸°í™”
        await pullDockerImage(image);
        expect(global.mockExec.exec).toHaveBeenCalledWith('docker', ['pull', image]);
