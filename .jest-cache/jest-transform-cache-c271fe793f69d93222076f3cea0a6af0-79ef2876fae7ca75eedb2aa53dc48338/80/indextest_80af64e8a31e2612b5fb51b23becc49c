      global.mockAxios.get.mockRejectedValueOnce(timeoutError);

      // Act & Assert
      if (getLatestTrivyVersion) {
        const version = await getLatestTrivyVersion();
        expect(version).toBe('0.49.1'); // ê¸°ë³¸ê°’ ë°˜í™˜
