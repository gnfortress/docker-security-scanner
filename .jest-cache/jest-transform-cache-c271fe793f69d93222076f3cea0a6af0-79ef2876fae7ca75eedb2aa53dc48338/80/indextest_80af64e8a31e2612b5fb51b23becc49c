  beforeEach(() => {
    // í…ŒìŠ¤íŠ¸ë³„ íŠ¹ë³„í•œ ì„¤ì •ì´ ìžˆë‹¤ë©´ ì—¬ê¸°ì— ì¶”ê°€
  });
  describe('ðŸš€ getLatestTrivyVersion', () => {
    test('ìµœì‹  Trivy ë²„ì „ì„ ì„±ê³µì ìœ¼ë¡œ ê°€ì ¸ì˜¨ë‹¤', async () => {
      // Arrange - setup.jsì˜ testUtils í™œìš©
      const expectedVersion = '0.49.1';
      global.mockAxios.get.mockResolvedValueOnce({
        data: {
          tag_name: `v${expectedVersion}`
        },
        status: 200
      });

      // Act
      const version = await getLatestTrivyVersion();

      // Assert
      expect(version).toBe(expectedVersion);
      expect(global.mockAxios.get).toHaveBeenCalledWith('https://api.github.com/repos/aquasecurity/trivy/releases/latest', {
        headers: {
          'User-Agent': 'GnFortress-Docker-Scanner'
        }
      });
    });
    test('API í˜¸ì¶œ ì‹¤íŒ¨ ì‹œ ê¸°ë³¸ê°’ì„ ë°˜í™˜í•œë‹¤', async () => {
      // Arrange
      global.mockAxios.get.mockRejectedValueOnce(new Error('Network error'));

      // Act
      const version = await getLatestTrivyVersion();

      // Assert
      expect(version).toBe('0.49.1');
      expect(global.mockCore.warning).toHaveBeenCalledWith(expect.stringContaining('Failed to get latest Trivy version'));
    });
    test('ë¹ˆ ì‘ë‹µ ì²˜ë¦¬', async () => {
      // Arrange
      global.mockAxios.get.mockResolvedValueOnce({
        data: {},
        status: 200
      });

      // Act
      const version = await getLatestTrivyVersion();

      // Assert
      expect(version).toBe('0.49.1'); // ê¸°ë³¸ê°’ ë°˜í™˜
    });
  });
  describe('ðŸ”§ installTrivy', () => {
    test('Trivyê°€ ì´ë¯¸ ì„¤ì¹˜ë˜ì–´ ìžˆìœ¼ë©´ ì„¤ì¹˜ë¥¼ ê±´ë„ˆë›´ë‹¤', async () => {
      // Arrange
      global.mockExec.exec.mockResolvedValueOnce(0); // trivy version ëª…ë ¹ ì„±ê³µ

      // Act
      await installTrivy();

      // Assert
      expect(global.mockExec.exec).toHaveBeenCalledWith('trivy', ['version'], {
        silent: true
      });
      expect(global.mockCore.info).toHaveBeenCalledWith('âœ… Trivy is already installed');
    });
    test('Linuxì—ì„œ Trivyë¥¼ ì„±ê³µì ìœ¼ë¡œ ì„¤ì¹˜í•œë‹¤', async () => {
      // Arrange
      Object.defineProperty(process, 'platform', {
        value: 'linux',
        writable: true,
        configurable: true
      });
      Object.defineProperty(process, 'arch', {
        value: 'x64',
        writable: true,
        configurable: true
      });
      global.mockExec.exec.mockRejectedValueOnce(new Error('trivy not found')) // ì²« ë²ˆì§¸ í˜¸ì¶œì€ ì‹¤íŒ¨
      .mockResolvedValue(0); // ë‚˜ë¨¸ì§€ í˜¸ì¶œì€ ì„±ê³µ

      global.mockAxios.get.mockResolvedValueOnce({
        data: {
          tag_name: 'v0.49.1'
        }
      });

      // Act
      await installTrivy('latest');

      // Assert
      expect(global.mockExec.exec).toHaveBeenCalledWith('curl', ['-L', '-o', 'trivy_0.49.1_Linux-amd64.tar.gz', 'https://github.com/aquasecurity/trivy/releases/download/v0.49.1/trivy_0.49.1_Linux-amd64.tar.gz']);
      expect(global.mockExec.exec).toHaveBeenCalledWith('tar', ['-xzf', 'trivy_0.49.1_Linux-amd64.tar.gz']);
    });
    test('macOSì—ì„œ Trivyë¥¼ ì„±ê³µì ìœ¼ë¡œ ì„¤ì¹˜í•œë‹¤', async () => {
      // Arrange
      Object.defineProperty(process, 'platform', {
        value: 'darwin',
        writable: true,
        configurable: true
