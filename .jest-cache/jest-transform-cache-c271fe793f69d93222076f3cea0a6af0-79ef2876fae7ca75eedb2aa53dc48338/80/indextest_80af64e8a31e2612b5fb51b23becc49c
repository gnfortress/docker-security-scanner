          tag_name: 'v0.49.1'
        }
      });

      // Act
      await installTrivy();

      // Assert
      expect(global.mockExec.exec).toHaveBeenCalledWith('curl', expect.arrayContaining([expect.stringContaining('trivy_0.49.1_macOS-amd64.tar.gz')]));
    });
    test('ì§€ì›ë˜ì§€ ì•ŠëŠ” í”Œëž«í¼ì—ì„œ ì—ëŸ¬ë¥¼ ë˜ì§„ë‹¤', async () => {
      // Arrange
